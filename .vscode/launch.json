{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run ft_vox",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/ft_vox",
      "args": ["-v", "1234"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build"
    }
  ]
}
